{
  "name": "jsonAPI",
  "main": "server.js",
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^@mock_models(.*)$": "<rootDir>/mocks/models$1",
      "^@routes(.*)$": "<rootDir>/routes$1",
      "^@handlers(.*)$": "<rootDir>/handlers$1"
    }
  },
  "_moduleAliases": {
    "@mock_models": "./mocks/models",
    "@routes": "./routes",
    "@handlers": "./handlers"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "faker": "^4.1.0",
    "module-alias": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
  }
}
